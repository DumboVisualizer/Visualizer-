 <div class="container-fluid bg">
    <div class="row">
        <div class="col-md-4 col-sm-4 col-xs-12"></div>
        <div class="col-md-4 col-sm-4 col-xs-12">
            <form class="form-container">
                <h1>Dumbo</h1>
                <div class="form-group">
                    <label id="username_color" for="usernameInput">Username</label>
                    <input type="username" class="form-control" id="usernameInput" placeholder="Username">
                </div>
                <div class="form-group">
                    <label id="pass_color" for="passwordInput">Password</label>
                    <input type="password" class="form-control" id="passwordInput" placeholder="Password">
                </div>
                <button type="button" class="btn btn-primary login-btn">Login</button>
                <button type="button" class="btn btn-primary register-btn">Register</button>
            </form>
        </div>
        <div class="col-md-4 col-sm-4 col-xs-12"></div>
    </div>
    <img id="dumbo_icon" src="/images/dumboicon.png" alt="Dumbo Icon">
 </div>

